{
  "name": "@massimo-cassandro/twig-utilities",
  "description": "Twig utilities for Symfony apps",
  "version": "3.0.0",
  "license": "MIT",
  "author": "Massimo Cassandro",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massimo-cassandro/twig-utilities.git#main"
  },
  "bugs": {
    "url": "https://github.com/twig-utilities/issues"
  },
  "homepage": "https://github.com/twig-utilities#readme",
  "keywords": [],
  "scripts": {
    "UPD-version": "npx update-version",
    "upd @m": "npm update @massimo-cassandro/linters-config @massimo-cassandro/dev-utilities @massimo-cassandro/js-utilities",
    "npm publish": "npm publish"
  },
  "browserslist": [
    "last 1 version",
    "iOS >= 13",
    "Safari >= 12",
    "not ie <= 11",
    "> 3% in IT"
  ],
  "stylelint": {
    "extends": "@massimo-cassandro/linters-config/stylelintrc.js"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@massimo-cassandro/linters-config/eslintrc.js"
    ],
    "ignorePatterns": []
  },
  "files": [
    "form-theme/**/*.*",
    "src/**/*.{scss,css,js,map}",
    "twig-shared-includes/**/*.*"
  ],
  "devDependencies": {
    "@massimo-cassandro/dev-utilities": "^1.9.4",
    "@massimo-cassandro/linters-config": "^1.2.5"
  },
  "dependencies": {
    "@massimo-cassandro/js-utilities": "^1.1.0"
  }
}